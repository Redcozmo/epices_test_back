<table data-toggle="table">
  <thead>
    <tr>
      <th>Onduleur</th>
      <th>Date</th>
      <th>Energie</th>
    </tr>
  </thead>
  <tbody>
    <% @daily_data.each do |daily_datum| %>
      <tr>
        <td><%= daily_datum.inverter.num %></td>
        <td><%= daily_datum.date %></td>
        <td><%= daily_datum.daily_energy %></td>
      </tr>
      <% end %>
  </tbody>
</table>

